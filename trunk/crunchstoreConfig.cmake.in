# - Config file for the crunchstore package
# It defines the following variables
#  crunchstore_INCLUDE_DIRS - include directories for crunchstore
#  crunchstore_LIBRARIES    - libraries to link against
#  crunchstore_EXECUTABLE   - the bar executable

@PACKAGE_INIT@

# These are IMPORTED targets created by crunchstoreLibraryDepends.cmake
set(crunchstore_LIBRARIES "Persistence" )
set(crunchstore_EXECUTABLE test)

# Compute paths
# get_filename_component(crunchstore_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
if(EXISTS "${PACKAGE_PREFIX_DIR}/CMakeCache.txt")
    # In build tree
    include("${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@/crunchstoreBuildTreeSettings.cmake")
    # Our library dependencies (contains definitions for IMPORTED targets)
    include("${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@/crunchstoreLibraryDepends.cmake")
else()
    set_and_check(crunchstore_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/include")
    # The crunchstore targets file.
    if(EXISTS "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@/crunchstore-targets.cmake")
        include("${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@/crunchstore-targets.cmake")
    endif()
endif()

# The location of the Usecrunchstore.cmake file.
set(crunchstore_USE_FILE "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@/Usecrunchstore.cmake")
